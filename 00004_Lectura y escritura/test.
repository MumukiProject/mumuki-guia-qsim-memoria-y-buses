examples:
- name: 'Se copia el valor del registro R4 a la posición de memoria 0xEEEF'
  preconditions:
    R4: '22FF'
  postconditions:
    equal:
      'EEEF': '22FF'
- name: 'Se copia el valor de la posición de memoria 0xEEEE al registro R3'
  preconditions:
    'EEEE': '4A5A'
  postconditions:
    equal:
      R3: '4A5A'
output:
  records: true
  memory:
    from: 'EEEE'
    to: 'EEFD'